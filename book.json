{
	"title": "Summary",
	"plugins" : [
		"expandable-chapters",
		"chapter-fold",  
		"github-buttons",
		"editlink",
		"copy-code-button",
		"page-footer-ex",
		"anchor-navigation-ex",
		"expandable-chapters-small",
		"prism", 
		"-highlight",
		"-lunr", 
		"-search", 
		"search-pro",
		"splitter",
		"code",
        "-search",
        "search-pro",
        "github",
		"popup",   
        "splitter",
        "-sharing",
        "sharing-plus",
        "-highlight",
        "styles-less",
        "toggle-chapters",
        "ancre-navigation"
	],
	"pluginsConfig": {
		"editlink": {
			"base": "https://github.com/Super-BUAA-2021/GinBook/blob/master",
			"label": "Edit This Page"
		},	
		"code": {
            "copyButtons": true
        },
		"github": {
            "url": "https://github.com/BFlameSwift"
        },
		"github-buttons": {
			"buttons": [{
				"user": "Super-BUAA-2021",
				"repo": "GinBook",
				"type": "star",
				"size": "small"
			}]
		},	
		"page-footer-ex": {
            "copyright": "By [BFlameSwift](https://github.com/BFlameSwift)，使用[知识共享 署名-相同方式共享 4.0协议](https://creativecommons.org/licenses/by-sa/4.0/)发布",
            "markdown": true,
            "update_label": "<i>updated</i>",
            "update_format": "YYYY-MM-DD HH:mm:ss"
		},	
		"prism": {
			"css": ["prismjs/themes/prism-solarizedlight.css"],
			"lang": {"flow": "typescript"}
		},
        "tbfed-pagefooter": {
            "copyright": "Copyright © BFlameSwift 2022",
            "modify_label": "本书发布时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
	},
	"ignores" : ["_book", "node_modules"]
}